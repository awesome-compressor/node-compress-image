{
  "name": "@awesome-compressor/node-image-compression",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.12.4",
  "description": "一个集成多个Node.js压缩库的通用图像压缩工具，自动选择最优压缩结果",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/awesome-compressor",
  "homepage": "https://github.com/awesome-compressor/node-image-compression#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awesome-compressor/node-image-compression.git"
  },
  "bugs": "https://github.com/awesome-compressor/node-image-compression/issues",
  "keywords": [
    "image",
    "compression",
    "compress",
    "optimize",
    "sharp",
    "jimp",
    "imagemin",
    "nodejs",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "run": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "npm run build -- --watch src",
    "format": "prettier --cache --write .",
    "lint": "eslint . --cache",
    "lint:fix": "npm run lint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:tools": "vitest run test/tools/",
    "test:tools-watch": "vitest test/tools/",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "example": "esno examples/basic.ts",
    "example:all": "esno examples/all-tools.ts"
  },
  "dependencies": {
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^8.0.0",
    "jimp": "^0.22.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@sxzz/prettier-config": "^2.2.1",
    "@types/imagemin": "^9.0.1",
    "@types/node": "^18.19.115",
    "bumpp": "^8.2.1",
    "eslint": "^9.30.1",
    "esno": "^4.8.0",
    "lint-staged": "^13.3.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^3.0.2",
    "tsdown": "^0.9.9",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  },
  "prettier": "@sxzz/prettier-config"
}
